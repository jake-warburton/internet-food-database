{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/no-danger": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "quotes": 0,
    "operator-linebreak": 0,
    "arrow-parens": 0
  }
}
