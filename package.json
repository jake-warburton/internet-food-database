{
  "name": "ifdb",
  "private": true,
  "version": "0.0.1",
  "description": "An online recipe web resource",
  "author": "Jake Warburton",
  "main": "index.js",
  "homepage": "http://91.206.183.182:3000/",
  "keywords": [
    "food",
    "cooking",
    "recipes"
  ],
  "license": "ISC",
  "scripts": {
    "dev": "node server.js",
    "build": "jest | next build",
    "start": "NODE_ENV=production node server.js",
    "readme": "npx readme",
    "eslint-init": "npx eslint --init",
    "eslint": "eslint --print-config . | eslint-config-prettier-check",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js",
      "*.jsx",
      "*.ts",
      "*.tsx"
    ],
    "coveragePathIgnorePatterns": [
      ".next",
      "/node_modules"
    ],
    "coverageTheshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "express": "^4.17.0",
    "next": "^9.3.3",
    "next-offline": "^4.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-easy-state": "^6.1.1",
    "react-icons": "^3.7.0",
    "react-lazy-load-image-component": "^1.3.2",
    "react-spring": "^8.0.25",
    "react-sticky-el": "^1.0.20"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "babel-jest": "^24.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "enzyme": "^3.4.3",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.1.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.11.1",
    "react-test-renderer": "^16.4.2",
    "readme-md-generator": "^0.2.1",
    "webpack-pwa-manifest": "^4.0.0"
  }
}
